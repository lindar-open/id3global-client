<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings
		xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="https://jakarta.ee/xml/ns/jaxb https://jakarta.ee/xml/ns/jaxb/bindingschema_3_0.xsd"
		version="3.0">

        	 <jaxb:globalBindings
					 generateElementProperty="false"
					 collectionType="java.util.ArrayList"
					 generateIsSetMethod="false">

				 <xjc:javaType adapter="com.lindar.id3global.adapter.LocalDateAdapter"
								name="java.time.LocalDate" xmlType="xs:date" />

				 <xjc:javaType adapter="com.lindar.id3global.adapter.InstantDateTimeAdapter"
							   name="java.time.Instant" xmlType="xs:dateTime"/>

			 </jaxb:globalBindings>


</jaxb:bindings>