<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings
		xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
		version="2.1">

        	 <jaxb:globalBindings
					 generateElementProperty="false"
					 collectionType="java.util.ArrayList"
					 generateIsSetMethod="false">

				 <xjc:javaType adapter="com.lindar.id3global.adapter.LocalDateAdapter"
								name="java.time.LocalDate" xmlType="xs:date" />

				 <xjc:javaType adapter="com.lindar.id3global.adapter.ZonedDateTimeAdapter"
							   name="java.time.ZonedDateTime" xmlType="xs:dateTime"/>

			 </jaxb:globalBindings>

	<jaxb:bindings schemaLocation="ID3global-xsd0.xml">
	    <jaxb:schemaBindings>
	        <jaxb:package name="com.lindar.id3global.schema"/>

	    </jaxb:schemaBindings>
	</jaxb:bindings>
	<jaxb:bindings schemaLocation="ID3global-xsd1.xml">
	    <jaxb:schemaBindings>
	        <jaxb:package name="com.lindar.id3global.schema.serialization"/>

	    </jaxb:schemaBindings>
	</jaxb:bindings>
	<jaxb:bindings schemaLocation="ID3global-xsd2.xml">
	    <jaxb:schemaBindings>
	        <jaxb:package name="com.lindar.id3global.schema.serialization.arrays"/>

	    </jaxb:schemaBindings>
	</jaxb:bindings>
	<jaxb:bindings schemaLocation="ID3global-xsd3.xml">
	    <jaxb:schemaBindings>
	        <jaxb:package name="com.lindar.id3global.schema.message"/>

	    </jaxb:schemaBindings>
	</jaxb:bindings>
	<jaxb:bindings schemaLocation="ID3global-xsd4.xml">
	    <jaxb:schemaBindings>
	        <jaxb:package name="com.lindar.id3global.schema.globalcheck.datalib"/>

	    </jaxb:schemaBindings>
	</jaxb:bindings>
</jaxb:bindings>